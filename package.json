{
  "name": "gatsby-blog",
  "private": true,
  "description": "A sample blog website built using Gatsby connected to Prismic CMS",
  "version": "0.1.0",
  "author": "Levi Gable",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "gatsby": "^2.1.32",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.9",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-source-prismic-graphql": "^3.1.3",
    "gulp": "^4.0.2",
    "node-sass": "^4.11.0",
    "prismic-javascript": "^2.0.1",
    "prismic-reactjs": "^0.3.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "reactstrap": "^8.0.1"
  },
  "devDependencies": {
    "prettier": "^1.16.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
